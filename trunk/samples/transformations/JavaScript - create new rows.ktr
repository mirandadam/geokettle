if (groupsField!=null) 
{
  var groups = groupsField.split(&quot;,&quot;);
 
  for (i=0;i&lt;groups.length;i++)
  {
    newRow = createRowCopy(getOutputRowMeta().size());
    var rowIndex = getInputRowMeta().size();

    newRow[rowIndex++] = trim( groups[i] );
    newRow[rowIndex++] = &quot;N&quot;;

    putRow(newRow);
  }
}

